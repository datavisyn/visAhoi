{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true
    },
    "extends": ["airbnb-base"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
      "no-underscore-dangle": "warn"
    }
}
