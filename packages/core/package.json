  "description": "Core library for the visualization onboarding",
  "keywords": [
    "visualization",
    "onboarding"
  ],
  "author": "FFG SEVA Consortium",
  "homepage": "https://github.com/datavisyn/visAhoi/tree/master/packages/core#readme",
