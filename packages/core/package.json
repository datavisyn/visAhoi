{
  "name": "@visahoi/core",
  "version": "2.0.0-SNAPSHOT",
  "description": "Core library for the visualization onboarding",
  "keywords": [
    "visualization",
    "onboarding"
  ],
  "author": "FFG SEVA Consortium",
  "homepage": "https://github.com/ffg-seva/onboarding-prototype/tree/master/packages/core#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ffg-seva/onboarding-prototype.git"
  },
  "bugs": {
    "url": "https://github.com/ffg-seva/onboarding-prototype/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json && npm run",
    "copy-css": "cp -r ./src/css/ ./dist/",
    "watch-plotly": "npm run compile -- --watch",
    "watch-echarts": "npm run compile -- --watch",
    "watch-vega": "npm run compile -- --watch"
  },
  "devDependencies": {
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@popperjs/core": "^2.9.3"
  }
}
